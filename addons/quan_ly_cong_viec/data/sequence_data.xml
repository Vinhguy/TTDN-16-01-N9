<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- NOTE: Sequence cho mã dự án đã được chuyển sang module project_management -->
        <!-- Model du_an đã bị xóa, sử dụng model projects thay thế -->

        <!-- Sequence cho mã công việc -->
        <record id="seq_cong_viec" model="ir.sequence">
            <field name="name">Mã công việc</field>
            <field name="code">cong_viec.sequence</field>
            <field name="prefix">CV</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <!-- Sequence cho mã nhiệm vụ -->
        <record id="seq_nhiem_vu" model="ir.sequence">
            <field name="name">Mã nhiệm vụ</field>
            <field name="code">nhiem_vu.sequence</field>
            <field name="prefix">NV</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <!-- Sequence cho mã tiến độ -->
        <record id="seq_tien_do" model="ir.sequence">
            <field name="name">Mã tiến độ</field>
            <field name="code">tien_do.sequence</field>
            <field name="prefix">TD</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>
    </data>
</odoo>
